<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".fragments.Admin.Schedule.Swapping.AdminScheduleSwappingUserActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">



        <EditText
            android:layout_width="match_parent"
            android:background="@drawable/teacher_recyclerview_background_cell"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:hint="Search"
            android:id="@+id/editText_admin_view_search"
            android:padding="10dp" />


    </LinearLayout>
    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Recycerview_Schedule_SwappingUser"/>
</LinearLayout>